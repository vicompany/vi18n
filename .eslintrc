{
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "id-length": 0,
    "indent": [2, "tab", { "SwitchCase": 1 }],
    "func-names": 0,
    "one-var": [2, {
      "var": "always",
      "let": "never",
      "const": "never"
    }],
    "padded-blocks": 0,
    "prefer-const": 0
  }
}
