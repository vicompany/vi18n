{
  "globals":  {
    "define"    : false,
    "require"   : false
  },

  "maxerr"      : 100,

  "bitwise"     : true,
  "camelcase"   : true,
  "curly"       : true,
  "eqeqeq"      : true,
  "forin"       : true,
  "immed"       : true,
  "latedef"     : true,
  "newcap"      : true,
  "noarg"       : true,
  "noempty"     : true,
  "nonew"       : true,
  "plusplus"    : false,
  "quotmark"    : "single",

  "undef"       : true,
  "unused"      : "vars",
  "trailing"    : true,
  "maxparams"   : 5,

  "browser"     : true,
  "devel"       : true,
  "esnext"      : true,
  "jasmine"     : true
}
